<h1>My Tasks</h1>

<p>
    <% @tasks.each do |task| %>
    <% if task.completed == true %>
    <p> [X] <%= link_to task.title, task_path(task.id)%> </p>
      <!-- <i class="far fa-check-square"></i> -->
    <% else %>
    <p> [ ] <%= link_to task.title, task_path(task.id) %> </p>
      <!-- <i class="far fa-square"></i> -->
    <% end %>
  <% end %>
</p>
